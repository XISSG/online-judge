                "user_password"
            ],
            "properties": {
                "avatar_url": {
                    "type": "string"
                },
                "user_name": {
                    "type": "string",
                    "maxLength": 64
                },
                "user_password": {
                    "type": "string",
                    "maxLength": 128
                }
            }
        }
