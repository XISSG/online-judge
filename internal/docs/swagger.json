                },
                "user_password": {
                    "type": "string"
                }
            }
        },
        "request.Question": {
            "type": "object"
        },
        "request.Submit": {
            "type": "object",
            "required": [
                "code",
                "language",
                "question_id"
            ],
            "properties": {
                "code": {
                    "description": "\"用户代码\"",
                    "type": "string",
                    "maxLength": 1024
                },
                "language": {
                    "description": "\"编程语言\"",
                    "type": "string",
                    "maxLength": 64
                },
                "question_id": {
                    "description": "\"判题id\"",
                    "type": "integer",
                    "maximum": 64
                }
            }
        },
        "request.UpdateQuestion": {
            "type": "object",
            "required": [
                "id",
                "user_id"
            ],
            "properties": {
                "accept_num": {
                    "description": "\"题目通过数\"",
                    "type": "integer"
                },
                "answer": {
                    "description": "\"题目答案,数组\"",
                    "type": "array",
                    "maxItems": 512,
                    "items": {
                        "type": "string"
                    }
                },
                "content": {
                    "description": "\"内容\"",
                    "type": "string",
                    "maxLength": 1024
                },
                "id": {
                    "type": "integer",
                    "maximum": 64
                },
                "judge_case": {
                    "description": "\"判题用例json数组,输入用例\"",
                    "type": "array",
                    "maxItems": 1024,
                    "items": {
                        "type": "string"
                    }
                },
                "judge_config": {
                    "description": "\"判题配置json对象,内存限制，时间限制\"",
                    "type": "array",
                    "maxItems": 64,
                    "items": {
                        "$ref": "#/definitions/common.Config"
                    }
                },
                "submit_num": {
                    "description": "\"题目提交数",
                    "type": "integer"
                },
                "tag": {
                    "description": "\"标签列表json数组\"",
                    "type": "array",
                    "maxItems": 128,
                    "items": {
                        "type": "string"
                    }
                },
                "title": {
                    "description": "\"标题\"",
                    "type": "string",
                    "maxLength": 512
                },
                "user_id": {
                    "description": "\"创建用户id\"",
                    "type": "integer",
                    "maximum": 64
                }
            }
        },
        "request.UpdateUser": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/request.Body"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "password",
                        "avatar"
                    ]
                }
            }
        },
        "request.User": {
            "type": "object",
            "required": [
